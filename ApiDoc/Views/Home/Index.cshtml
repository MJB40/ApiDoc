@{
    ViewData["Title"] = "";
}

@section StyleSheet
{
    <link rel="stylesheet" href="~/css/home.css" />

}

<div class="absolute-left-58  w-240 bg-light border-right">
    <div class="input-group input-group-sm article-search">
        <input type="text" class="form-control" placeholder="Search..." aria-label="Search" aria-describedby="basic-addon2" autocomplete="off">
        <div class="input-group-append">
            <span class="input-group-text" id="basic-addon2"><i class="zi zi_searchBlack"></i></span>
        </div>
    </div>
    <div id="treeview7"></div>
</div>

<div class="absolute-right-58 w-width-240" style="padding:0;">
 
            <iframe id="myIframeList" src="~/Interface/Index" frameborder="0" scrolling="yes" style="width:100%;height: 100%;"></iframe>
        
</div>
<input type="hidden" />

<div class="modal fade bd-example-modal-xl" id="myModalInterface" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-title bg-light text-right">
                <button type="button" class="btn btn-default" data-dismiss="modal"><span class="fa fa-close" aria-hidden="true"></span></button>
            </div>
            <div class="modal-body p-0">
                <iframe id="myIframe" src="" frameborder="0" scrolling="yes" style="width:100%;height: 100%;"></iframe>
            </div>
            
        </div>
    </div>
</div>

@section Scripts
{
    <script src="~/js/bootstrap.bundle.js"></script>
    <script src="~/js/bootstrap-treeview.js"></script>
    <script src="~/js/bootstrap-closable-tab.js"></script>
    <script src="~/js/home.js"></script>

}


