@{
    ViewData["Title"] = "Home Page";
}

@section StyleSheet
{
    <link rel="stylesheet" href="~/css/home.css" />

}

<div class="absolute-left-58  w-240 bg-light border-right">
    <div class="input-group input-group-sm article-search">
        <input type="text" class="form-control" placeholder="Search..." aria-label="Search" aria-describedby="basic-addon2" autocomplete="off">
        <div class="input-group-append">
            <span class="input-group-text" id="basic-addon2"><i class="zi zi_searchBlack"></i></span>
        </div>
    </div>
    <div id="treeview7"></div>
</div>

<div class="absolute-right-58 w-width-240" style="padding:0;">

    <ul class="nav nav-tabs px-lg-4" id="myTab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="home" aria-selected="true">接口列表</a>
        </li>
    </ul>

    <div class="tab-content tabContent" style="margin:0;" id="myTabContent">
        <div class="tab-pane fade h-100  show active " id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
            <iframe src="~/Interface/Index" frameborder="0" scrolling="yes" style="width:100%;height: 100%;"></iframe>
        </div>
    </div>

</div>


@section Scripts
{
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap-treeview.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap-closable-tab.js"></script>
    <script src="~/js/home.js"></script>
   
}


