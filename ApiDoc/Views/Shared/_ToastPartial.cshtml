 
<div id="myToastWarning" class="toast position-fixed " role="alert" aria-live="assertive" aria-atomic="true" style="top:50%; left:50%; z-index:5;">
    <div class="toast-header bg-warning ">
        <i class="fa fa-info"></i>
        <strong class="mr-auto"></strong>
        <small></small>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div id="myToastWarningText" class="toast-body p-5 font-weight-bold">
        请选择要修改的数据
    </div>
</div>

<div id="myToastSuccess" class="toast position-fixed " role="alert" aria-live="assertive" aria-atomic="true" style="top:50%; left:50%; z-index:5;">
    <div class="toast-header bg-primary text-white">
        <i class="fa fa-info"></i>
        <strong class="mr-auto"></strong>
        <small></small>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div id="myToastSuccessText" class="toast-body  text-success  p-5 font-weight-bold">
        保存成功
    </div>
</div>

<script type="text/javascript">

    function popToastWarning(msg) {
       
        var myToastWarning = $("#myToastWarning");
        $("#myToastWarningText").html(msg); 
        myToastWarning.toast("show");
    }

    function popToastSuccess(msg) {
        var myToastWarning = $("#myToastSuccess");
        $("#myToastSuccessText").html(msg);
        myToastWarning.toast("show");
    }

</script>